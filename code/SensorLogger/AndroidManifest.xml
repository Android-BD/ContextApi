<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="uk.co.md87.android.sensorlogger" android:versionCode="8" android:versionName="0.1.2">
    <application android:label="Sensor Logger">
         <activity android:name=".MainActivity" android:label="Sensor Logger">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:name=".SensorLoggerService" android:label="Sensor Logger Service"/>
        <service android:name=".UploaderService" android:label="Sensor Uploader Service"/>
    </application>

    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
